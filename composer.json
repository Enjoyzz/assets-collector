{
    "name": "enjoys/assets-collector",
    "description": "Assets Collector CSS and JS",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Enjoys",
            "email": "deadly.info@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/log": "^1.0 | ^2.0 | ^3.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "enjoys/url-converter": "*",
        "enjoys/functions": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Enjoys\\AssetsCollector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Enjoys\\AssetsCollector\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^6.0",
        "phpunit/phpunit": "^9.5",
        "twig/twig": "^3.2",
        "vimeo/psalm": "^5.0",
        "guzzlehttp/guzzle": "^7.2",
        "tubalmartin/cssmin": "^4.1",
        "tedivm/jshrink": "^1.6.2 | ~1.5.0",
        "roave/infection-static-analysis-plugin": "^1.33"
    },
    "scripts": {
        "psalm": "psalm --show-info=true --config=psalm.xml --no-cache",
        "infection": "@php vendor/bin/roave-infection-static-analysis-plugin -j2"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
